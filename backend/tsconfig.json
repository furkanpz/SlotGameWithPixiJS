{
  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "out",
    "rootDir": "",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node", "aws-lambda"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
